    "gulp-util": "^3.0.7",
    "html-webpack-plugin": "^2.24.1",
    "mocha": "^3.1.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "sinon": "^1.17.6",
    "sinon-mongoose": "^1.3.0",
    "style-loader": "^0.13.1",
