    "browser-sync": "^2.18.2",
    "chai": "^3.5.0",
    "eslint": "^3.12.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^2.1.14",
