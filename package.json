    "url-loader": "^0.5.7",
    "webpack-cleanup-plugin": "^0.4.1",
    "webpack-dev-server": "^1.16.2"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0
    }
  }
}
